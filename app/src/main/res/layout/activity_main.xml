<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/kenny9"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:contentDescription="@string/app_name"
        android:onClick="clicked"
        app:layout_constraintStart_toEndOf="@+id/kenny8"
        app:layout_constraintTop_toBottomOf="@+id/kenny6"
        app:srcCompat="@drawable/kenny" />

    <ImageView
        android:id="@+id/kenny8"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:onClick="clicked"
        android:contentDescription="@string/app_name"
        app:layout_constraintStart_toEndOf="@+id/kenny7"
        app:layout_constraintTop_toBottomOf="@+id/kenny5"
        app:srcCompat="@drawable/kenny" />

    <ImageView
        android:id="@+id/kenny7"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="16dp"
        android:contentDescription="@string/app_name"
        android:onClick="clicked"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/kenny4"
        app:srcCompat="@drawable/kenny" />

    <ImageView
        android:id="@+id/kenny6"
        android:layout_width="120dp"
        android:contentDescription="@string/app_name"
        android:layout_height="120dp"
        android:onClick="clicked"
        app:layout_constraintStart_toEndOf="@+id/kenny5"
        app:layout_constraintTop_toBottomOf="@+id/kenny3"
        app:srcCompat="@drawable/kenny" />

    <ImageView
        android:id="@+id/kenny5"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:contentDescription="@string/app_name"
        android:onClick="clicked"
        app:layout_constraintStart_toEndOf="@+id/kenny4"
        app:layout_constraintTop_toBottomOf="@+id/kenny2"
        app:srcCompat="@drawable/kenny" />

    <ImageView
        android:id="@+id/kenny4"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="16dp"
        android:contentDescription="@string/app_name"
        android:onClick="clicked"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/kenny1"
        app:srcCompat="@drawable/kenny" />

    <ImageView
        android:id="@+id/kenny3"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="192dp"
        android:contentDescription="@string/app_name"
        android:onClick="clicked"
        app:layout_constraintStart_toEndOf="@+id/kenny2"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/kenny" />

    <ImageView
        android:id="@+id/kenny2"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="192dp"
        android:contentDescription="@string/app_name"
        android:onClick="clicked"
        app:layout_constraintStart_toEndOf="@+id/kenny1"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/kenny" />

    <TextView
        android:id="@+id/tvScore"
        android:layout_width="359dp"
        android:layout_height="37dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:text="TextView"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvTime" />

    <TextView
        android:id="@+id/tvTime"
        android:layout_width="365dp"
        android:layout_height="51dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="16dp"
        android:text="TextView"
        android:textAlignment="center"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvHighScore"
        android:layout_width="370dp"
        android:layout_height="38dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="32dp"
        android:text="TextView"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/kenny1"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="16dp"
        android:contentDescription="@string/app_name"
        android:layout_marginTop="192dp"
        android:onClick="clicked"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/kenny" />

</androidx.constraintlayout.widget.ConstraintLayout>